#include <iostream>

using namespace std;

long long pow(long long a, long long b){
if (b<=1)
    return a;
if (b%2==0){
    a*=a;
    b/=2;
    pow(a,b);
} else {
    a=a*pow(a,b-1);
}
}
int main()
{   long long number,amount,answer;
    cin >> number >> amount;
    answer=pow(number,amount);
    cout << answer;
    return 0;
}
